{
	"name": "qubo-frontend",
	"version": "1.0.0",
	"private": true,
	"workspaces": ["apps/*", "features/*"],
	"scripts": {
		"prettier": "prettier --write --config ./.prettierrc.json \"**/*.{js,jsx,ts,tsx,scss,json}\"",
		"eslint": "eslint --config ./.eslintrc.json \"**/*.{js,jsx,ts,tsx}\" --fix",
		"stylelint": "stylelint \"**/*.{css,scss}\" --fix",
		"lint": "npm-run-all eslint prettier stylelint"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"classnames": "^2.3.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"devDependencies": {
		"@types/react": "^17.0.2",
		"@types/react-dom": "^17.0.2",
		"@typescript-eslint/eslint-plugin": "^4.22.0",
		"@typescript-eslint/parser": "^4.22.0",
		"eslint": "^7.24.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-prettier": "^8.2.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-react": "^7.23.2",
		"eslint-plugin-react-hooks": "^4.2.0",
		"lint-staged": "^10.5.4",
		"npm-run-all": "^4.1.5",
		"prettier": "2.2.1",
		"sass": "^1.32.11",
		"stylelint": "^13.12.0",
		"stylelint-config-prettier": "^8.0.2",
		"stylelint-config-standard": "^21.0.0",
		"typescript": "^4.2.4"
	},
	"resolutions": {
		"@types/react": "^17.0.2",
		"qubo-frontend": "workspace:*"
	}
}
